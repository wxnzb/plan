//4
#include<stdio.h>
int main()
{
  float a;
  scanf("%f",&a);
  printf("Enter a floating-point value: %.2f\n",a);
  printf("fixed-point notation: %f\n",a);
  printf("exponential notation: %e\n",a);
  printf("p notation: %a",a);
  return 0;
}
