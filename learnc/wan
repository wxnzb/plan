#include<stdio.h>
void smile(void);
int main()
{
  smile();smile();smile();
  printf("\n");
  smile();smile();
  printf("\n");
  smile();
  printf("\n");
  return 0;
}
void smile(void)
{
  printf("Smile!");
}
//8
#include<stdio.h>
void one_three(void);
void two(void);
int main()
{
  one_three();
  two();
  printf("done!");
  return 0;
}
void one_three(void)
{
  printf("starting now;\n");
}
void two(void)
{
  printf("one;\ntwo;\nthree;\n");
}
