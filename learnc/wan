//1
#include<stdio.h>
int main()
{
  long num;
  long sum=0l;
  int status;
  printf("Please enter an interqer to be summed");
  status=scanf("%d",&num);
  while(status==1)
  {
    printf("Please enter an interqer to be summed");
    sum+=num;
    status=scanf("%ld",&num);
  }
  printf("Those interqers sum to %d",sum);
  return 0;
}
