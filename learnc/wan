#include<stdio.h>
int main()
{
  int a,b;
  a=5;
  b=2;//è¡¨ç¤ºæŠŠ2èµ‹ç»™äº†b
  b=a;//æŠŠaçš„å€¼5èµ‹ç»™äº†b
  a=b;//æŠŠbçš„5èµ‹ç»™äº†a
  printf("%d %d\n",b,a);
  return 0;
}
