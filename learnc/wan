//6
#include<stdio.h>
#include<string.h>
int main()
{
  char name[20];
  char surname[20];
  scanf("%s %s",name,surname);
  printf("%s %s\n",name,surname);
  printf("%*d %*d\n",strlen(name),strlen(name),strlen(surname),strlen(surname));
  printf("%s %s\n",name,surname);
  printf("%-*d %-*d",strlen(name),strlen(name),strlen(surname),strlen(surname));
  return 0;
}
